{
    "Service": { "Name": "CloudGames.Users" },
    "ConnectionStrings": {
        "UsersDb": "Server=localhost;Database=CloudGamesUsers;User Id=sa;Password=YourPassword;TrustServerCertificate=True;",
        "Storage": "UseDevelopmentStorage=true"
    },
    "Queues": { "Users": "users-events" },
    "JwtSettings": {
        "Secret": "my-super-secret-key-1234567890-ABCD",
        "Issuer": "CloudGames",
        "Audience": "CloudGamesUsers"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        }
    }
}

